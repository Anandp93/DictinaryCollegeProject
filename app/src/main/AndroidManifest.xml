        <activity
            android:name="com.example.dictinarycollegeproject.AptitudeActivity5"
            android:label="@string/title_activity_aptitude_activity5" >
        </activity>
        <activity
            android:name="com.example.dictinarycollegeproject.ResultActivity"
            android:label="@string/title_activity_result" >
        </activity>
    </application>

</manifest>